.thumb{
    margin-bottom: 30px;
}
        
.page-top{
    margin-top:80px;
}
   
img.zoom {
    width: 100%;
    height: 200px;
    border-radius:5px;
    object-fit:cover;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    -ms-transition: all .3s ease-in-out;
}
        
 
.transition {
    -webkit-transform: scale(1.1); 
    -moz-transform: scale(1.1);
    -o-transform: scale(1.1);
    transform: scale(1.1);
}

#upload-btn{
		display: none;
}	

.arrow-keys{
	  top: 50px;
    position: relative;
}

/* Progressbar */

.ui-progressbar {
    position: relative;
}

.progress-label {
    position: absolute;
    left: 45%;
    top: 4px;
    font-weight: bold;
    text-shadow: 1px 1px 0 #fff;
}

#progressbar{
	display: inline-block;
  width: 90%;
}

#loginModal{
	background-color: rgba(26, 29, 32, 0.8);
}

.login-register-dialog{
	min-height: 100%;
}

.modal-dialog{
	position: relative;
  top: 10vh;
	margin: 0 auto;
}

.form-group.py-4{
	margin: 0;
}


.formCheck{
	display: none;
}

.custom-checkbox{
	margin-bottom: 30px;
	margin-left: 15px;
}

.container.gallery-container{
	background-color: #fff;
  color: #35373a;
  min-height: 100vh;
  border-radius: 20px;
  box-shadow: 0 8px 15px rgba(0, 0, 0, 0.06);
}

.gallery{
	padding: 0;
	margin: 15px;
}

.buttonImgShow a{
  position: absolute;
  left: 32px;
  top: 140px;
  text-align: center;
  opacity: 0;
  transition: opacity .35s ease;
}

.buttonImgShow a, .buttonMusicPlay a, .buttonVideoPlay a {
  width: 200px;
  padding: 12px 48px;
  text-align: center;
  color: white;
  border: solid 2px white;
  z-index: 1;
}

.gallery:hover .buttonImgShow {
  opacity: 1;
}

.logout-icon:hover{
	cursor: pointer;
