html{
	height: 100%;
}

#emergency_stop{
	border-radius: 0px;
	position: relative;
	top: 0px;
	left: 240px;
	height: 100%;
	margin: 0px 15px;
}


body{
	width: 100%;
	height: 100%;
	background-color: #EEF5F9;
	padding: 0;
}

.navbar-nav .nav-item{
   	margin-left: 15px;
   	display: inline;
}

ul, li{
	list-style: none;
}

.navbar{
   	padding-top: 0px;
    padding-bottom: 0px;
    box-shadow: 3px 0 3px 3px rgba(0,0,0,0.25);
    min-height: 70px;
}

.navbar-brand{
   	display: inline;
}

.system-icons{
    font-size: 1.4rem;
    color: white;
}

.nav-link:hover{
    background-color: rgb(51, 133, 255);
    cursor: pointer;
	transition: all .5s ease;
	height: 100%;
	color: white;
}

.navbar-nav2{
   	list-style: none;
   	padding: 0px;
}

.navbar-nav{
   	flex-direction: row;
}

/* END OF NAVBAR */


#sidebar {
  overflow: auto;
  font-family: verdana;
  font-size: 14px;
  font-weight: 400;
  background-color: #2e353d;
  position: fixed;
  left: 0px;
  height: 100%;
  color: #e1ffff;
  padding: 0px;
  width: 240px; 
}

.sidebar-brand{
	text-align: center;
	padding: 15px;
	font-weight: 350;
}

.menu-content{
    padding: 0px;
}

.menu-content > li{
	margin-bottom: 10px;
    margin-top: 10px;
    padding: 7px;
}

.menu-content > li > a {
    text-decoration: none;
}

.menu-content > li > a:after{ 
	content: "+";
	float: right;
}

.sidebar-link{
	display: block;
}

.menu-content > li:hover{
	background-color: lightgrey;
}

.sub-menu{
	display: none;
}
	
.sub-menu > li > a{
	color: white;
	text-decoration: none;
}

.sub-menu > li > a:hover:before{
	content: "> ";
}

/* END OF SIDEBAR */

.wrapper{
	/*position: relative;*/
	top: 70px;
}

/* CONTENT */
.content{
	position: relative;
	top: 15px;
}

.content > a{
	margin-left: 20px;
}

.content-header {
	position: relative;
	top: 0px;
	border: 1px solid rgba(0,0,0,0.1);
	padding: 10px;
	margin: 10px;
	border-radius: 5px;
	background-color: white;
}

.content-container{
	position: relative;
    border: 1px solid #ccc;
    margin: 10px;
    padding: 10px;
    border-radius: 4px;
    background-color: white;
}

@media screen and (min-width: 992px) {
	.content{
		left: 235px;
	}
	.col-lg-9{
		left: 270px;
    position: relative;
    flex: 0 0 80%;
    max-width: 80%;
	}
}

label{
	font-size: 1.25em;
	letter-spacing: 1px;
}

#scriptSign{
	font-weight: bold;
	margin-right: 10px;
	float: right;
	display: inline-block;
}

#header-title{
	display: inline-block;
}

input[type=text], textarea {
    border: 3px solid #ccc;
    transition: 0.5s;
    border-radius: 4px;
}

@media screen and (max-width: 768px) {
	.navbar-brand{
		font-size: 1rem;
	}
	
	.scripts-view{
		text-align: center;
	}
}
/* view_scripts styling */

.scripts-view{
	margin-top: 20px;
}
	
.script-view-link{
	display: inline-block;
	margin-right: 30px;
}

.script-image{
	height: 180px;
}

