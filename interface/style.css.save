input[type=text]:focus {
    border: 3px solid #555;
}



.gallery-scripts{
	position: relative;
	margin: 10px;
}

.gallery-scripts > a > img{
	height: 150px;
	width: 150px;
	transition: transform .2s;
}

.gallery-scripts > img:hover{
	transform: scale(1.05);
	cursor: pointer;
}

.gallery-scripts-content{
	position: relative;
    display: flex;
    flex-wrap: wrap;
    top: 70px;
    width: 80%;
    margin: 0 auto;
    left: 8%;
}

.script-name{
	padding: 15px;
	text-align: center;
	font-weight: bold;
}

.edit-script{
	display: none;
	position: relative;
    top: 150px;
    left: 20%;
    width: 80%;
    margin: 0 auto;
}

.goback{
	position: relative;
	background-color: #DDE4E8;
    left: 20%;
}

.edit-script-btn{
	display: none;
	position: relative;
    top: 100px;
    left: 50%;
    width: 10%;
    text-align: center;
}

.uploader{
	position: relative;
	top: 100px;
	left: 20%;
}

.modal, .modal-img {
    display: none; /* Hidden by default */
    position: fixed; /* Stay in place */
    z-index: 1; /* Sit on top */
    padding-top: 100px; /* Location of the box */
    left: 0;
    top: 0;
    width: 100%; /* Full width */
    height: 100%; /* Full height */
    overflow: auto; /* Enable scroll if needed */
    background-color: rgb(0,0,0); /* Fallbackf color */
    background-color: rgba(0,0,0,0.9); /* Black w/ opacity */
}

.modal-content{
	width: 80%;
	margin: 0 auto;
}

.close1, .close2{
	color: white;
}

#deleteStatus{
	position: fixed;
  width: 60%;
  left: 25%;
  top: 150px;
	z-index: 5;
	//display: none;
}

#no-files-found{
	position: relative;
    left: 20px;
    top: 20px;
}

.arrows{
	border-radius: 6px;
	border: 2px solid black;
	padding: 20px;
	margin: 20px;
	text-align: center;
	background-color: lightblue;
    width: 80px;
	height: 80px;
	display: inline-block;
	position: relative;
}
#up{
	position: relative;
	margin: 0 auto;
}

#wrapper{
	position: relative;
	margin: 0 auto; 
}

.row{
	/*margin: 0;*/
	padding: 0;
}

.main.row{
	position: relative;
	top: 70px;
}

#keyboard_control{
	margin: 0 auto;
}

